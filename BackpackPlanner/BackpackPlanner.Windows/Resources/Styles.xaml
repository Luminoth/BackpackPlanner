<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BackpackPlannerPrimary" Color="#668000" />

    <Style x:Key="BackpackPlannerPage" TargetType="Panel">
        <Setter Property="Background" Value="#DECD87" />
    </Style>

    <Style x:Key="BackpackPlannerControl" TargetType="Control">
        <Setter Property="Background" Value="#501616" />
        <Setter Property="Foreground" Value="White" />
    </Style>
    <SolidColorBrush x:Key="BackpackPlannerButtonMouseOverForeground" Color="White" />

<!-- http://marcangers.com/material-design-shadows-in-wpf/ -->
<!-- http://materialdesigninxaml.net/ -->

</ResourceDictionary>
